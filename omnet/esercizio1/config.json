{
    "scenario_schema": {
        "p": {"pattern": "**.p", "type": "real"} 
    },
    "metrics": {
        "ResponseTime": {"module": "**.sink", "scalar_name": "lifeTime:mean" ,"aggr": ["none"]}
    },
    "histograms": {
        "ResponseTimeHistogram": {"module": "**.sink", "histogram_name": "lifeTime:histogram"}
    },
    "analyses": {
        "Hist_RT": {
            "outfile": "analisi/histogram.data",
            "scenario": {"p": "0.7"},
            "histogram": "ResponseTimeHistogram"
        },
        "es1": {
            "outfile": "analisi/es1.data",
            "scenarios": {
                "fixed": { "p": "0.7" },
                "range": []
            },
            "metrics": [
                {"metric": "ResponseTime", "aggr": "none"}
                    ]
        },
        "es3":{
            "outfile":"analisi/es3.data",
            "scenarios": {
                "fixed": {},
                "range": ["p"]
            },
            "metrics": [{"metric": "ResponseTime", "aggr": "none"}]
        }

    }
}